{
  "name": "wyrihaximus/string-get-in",
  "description": "Wrapper around igorw/get-in using strings rather then arrays are keys",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com"
    }
  ],
  "require": {
    "php": "^8 || ^7.4",
    "igorw/get-in": "^1.0.3"
  },
  "require-dev": {
    "wyrihaximus/test-utilities": "^3.7.6"
  },
  "config": {
    "platform": {
      "php": "7.4.7"
    },
    "sort-packages": true
  },
  "autoload": {
    "files": [
      "src/functions_include.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "WyriHaximus\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
